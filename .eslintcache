, useParams } from \"react-router-dom\";\n\n// API\nimport { createProduct, updateProduct, getProduct } from \"src/data/Interface\";\n\n// Layout\nimport {\n  Button,\n  Input,\n  Heading,\n  Fade,\n  Center,\n  Spinner,\n} from \"@chakra-ui/react\";\n\n// Interfaces\nimport { IVideo } from \"src/ts/interfaces\";\n\ntype FormElement = React.FormEvent<HTMLFormElement>;\n\ninterface IUserId {\n  id: string;\n}\n\nconst Form: React.SFC = () => {\n  const initialData = {\n    title: \"\",\n    description: \"\",\n    url: \"\",\n  };\n\n  // State\n  const [data, setData] = useState<IVideo>(initialData);\n  const [isEditVideo, setIsEditVideo] = useState<boolean>(false);\n  const [loading, setLoading] = useState<boolean>(true);\n\n  const { title, description, url } = data;\n  const validateData = title && url;\n\n  // Router\n  const { push, location } = useHistory();\n  const { pathname } = location;\n  const { id } = useParams<IUserId>();\n\n  const handleSubmit = async (e: FormElement) => {\n    e.preventDefault();\n    if (validateData) {\n      try {\n        if (!isEditVideo) {\n          await createProduct(data);\n        } else {\n          await updateProduct(id, data);\n        }\n        setData(initialData);\n        push(\"/\");\n      } catch (error) {\n        console.error(\"Error create video ->\", error);\n      }\n    }\n  };\n\n  const handleGetVideo = async (): Promise<void> => {\n    setLoading(true);\n    try {\n      const { data: d } = await getProduct(id);\n      setData({ title: d?.title, description: d?.description, url: d?.url });\n      setLoading(false);\n    } catch (e) {\n      console.error(\"Error Get Videos ->\", e);\n      setLoading(false);\n    }\n  };\n\n  useEffect(() => {\n    if (pathname) {\n      if (pathname.includes(\"/editar-video/\")) {\n        setIsEditVideo(true);\n        handleGetVideo();\n      }\n    }\n  }, [pathname]);\n\n  if (loading)\n    return (\n      <Center w=\"100%\" h=\"60vh\">\n        <Spinner w=\"3rem\" h=\"3rem\" />\n      </Center>\n    );\n\n  return (\n    <Fade in>\n      <form onSubmit={handleSubmit}>\n        <Heading as=\"h1\" mb={4}>\n          {!isEditVideo ? \"Nuevo vídeo\" : \"Editar vídeo\"}\n        </Heading>\n        <Input\n          type=\"text\"\n          placeholder=\"Título\"\n          isRequired\n          value={title}\n          onChange={(e) => setData({ ...data, title: e.target.value })}\n          mb={3}\n        />\n        <Input\n          type=\"text\"\n          placeholder=\"Descripción\"\n          isRequired\n          value={description}\n          onChange={(e) => setData({ ...data, description: e.target.value })}\n          mb={3}\n        />\n        <Input\n          type=\"url\"\n          placeholder=\"URL del vídeo\"\n          isRequired\n          value={url}\n          onChange={(e) => setData({ ...data, url: e.target.value })}\n          mb={3}\n        />\n        <Button\n          colorScheme=\"blue\"\n          type=\"submit\"\n          mt={5}\n          disabled={!validateData}\n        >\n          Guardar\n        </Button>\n      </form>\n    </Fade>\n  );\n};\n\nexport default Form;\n","/mnt/c/Users/JuanB/desktop/mern-typescript/frontend/src/data/Interface.ts",[],"/mnt/c/Users/JuanB/Desktop/mern-typescript/frontend/src/pages/Form.tsx",[],"/mnt/c/Users/JuanB/Desktop/mern-typescript/frontend/src/data/Interface.ts",[],{"ruleId":"60","severity":1,"message":"61","line":53,"column":7,"nodeType":"62","messageId":"63","endLine":53,"endColumn":20},{"ruleId":"60","severity":1,"message":"61","line":69,"column":7,"nodeType":"62","messageId":"63","endLine":69,"endColumn":20},{"ruleId":"60","severity":1,"message":"61","line":52,"column":7,"nodeType":"62","messageId":"63","endLine":52,"endColumn":20},{"ruleId":"60","severity":1,"message":"61","line":68,"column":7,"nodeType":"62","messageId":"63","endLine":68,"endColumn":20},{"ruleId":"60","severity":1,"message":"61","line":59,"column":9,"nodeType":"62","messageId":"6